<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17">
   <children>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label text="이름" />
            <TextField fx:id="name" prefHeight="22.0" prefWidth="62.0" promptText="이름입력" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox spacing="10.0">
         <children>
            <Label text="국어" />
            <TextField fx:id="korean" prefHeight="22.0" prefWidth="62.0" promptText="이름입력" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox spacing="10.0">
         <children>
            <Label text="수학" />
            <TextField fx:id="math" prefHeight="22.0" prefWidth="62.0" promptText="이름입력" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox spacing="10.0">
         <children>
            <Label text="영어" />
            <TextField fx:id="english" prefHeight="22.0" prefWidth="62.0" promptText="이름입력" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Button fx:id="btnSave" mnemonicParsing="false" text="저장" />
            <Button mnemonicParsing="false" text="취소" />
         </children>
         <VBox.margin>
            <Insets fx:id="btnCancel" bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
